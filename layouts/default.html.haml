!!!
/[if IE 7] <html class="ie7 no-js" lang="en">
/[if lte IE 8] <html class="ie8 no-js" lang="en">
/ [if (gte IE 9)|!(IE)]><!
%html.not-ie.no-js{:lang => "en"}
  / <![endif]
  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width, initial-scale=1, maximum-scale=1", :name => "viewport"}
    %title Meet Our Team - SmartStart
    %meta{:content => "", :name => "description"}
    %meta{:content => "", :name => "author"}
    / [if !lte IE 6]><!
    %link{:href => "/stylesheet/style.css", :media => "screen", :rel => "stylesheet"}/
    %link{:href => "//fonts.googleapis.com/css?family=Open+Sans:400,600,300,800,700,400italic|PT+Serif:400,400italic", :rel => "stylesheet"}/
    %link{:href => "css/fancybox.min.css", :media => "screen", :rel => "stylesheet"}/
    %link{:href => "css/video-js.min.css", :media => "screen", :rel => "stylesheet"}/
    %link{:href => "css/audioplayerv1.min.css", :media => "screen", :rel => "stylesheet"}/
    / <![endif]
    /[if lte IE 6]
      <link rel="stylesheet" href="//universal-ie6-css.googlecode.com/files/ie6.1.1.css" media="screen, projection">
    / HTML5 Shiv + detect touch events
    %script{:src => "js/modernizr.custom.js"}
    / HTML5 video player
    %script{:src => "js/video.min.js"}
    :javascript
      _V_.options.flash.swf = 'http://localhost/smartstart/js/video-js.swf';  
  %body
    %header#header.container.clearfix
      %a#logo{:href => "index.html"}
        %img{:alt => "SmartStart", :src => "img/logo.png"}
      = render 'nav'
    / end #header
    %section#content.container.clearfix
      = yield
    / end #content
    = render 'footer'

    / [if !lte IE 6]><!
    %script{:src => "//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"}
    :javascript
      window.jQuery || document.write('<\/script>')
